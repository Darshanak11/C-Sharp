using System;
class animal
{
    public void Eat()
    {
        Console.WriteLine("animal eating");
    }
    class Dog : animal
    {
        public void bark()
        {
            Console.WriteLine("Dog barking");
        }

    }
    class program
    {
        static void Main()
        {
            Dog A1 = new Dog();
            A1.bark();
            A1.Eat();
        }
    }
}

